; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="godot-4.4-xr-template"
run/main_scene="uid://cbvmuhvb57kqq"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

XRToolsUserSettings="*res://addons/godot-xr-tools/user_settings/user_settings.gd"
XRToolsRumbleManager="*res://addons/godot-xr-tools/rumble/rumble_manager.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-xr-tools/plugin.cfg")

[filesystem]

import/blender/enabled=false

[layer_names]

3d_physics/layer_1="Static World - for fixed scenery objects that never move"
3d_physics/layer_2="Dynamic World - for scenery objects which may move"
3d_physics/layer_3="Pickable Objects - for pickable objects"
3d_physics/layer_4="Wall Walking Surface - for objects the player can wall walk on"
3d_physics/layer_5="Grappling Target - for grappling movement"
3d_physics/layer_17="Held Objects - for pickable objects held by the player"
3d_physics/layer_18="Player Hand - for the players physical hand bones"
3d_physics/layer_19="Grab handles - for pickable objects"
3d_physics/layer_20="Player Body - for the players body"
3d_physics/layer_21="Pointable - for objects the player can interact with using the pointer"
3d_physics/layer_22="Hand Pose Area - for areas enforcing hand poses"
3d_physics/layer_23="UI Objects - for physically touchable UIs using the pointer or finger poke"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true

[xr]

openxr/enabled=true
openxr/foveation_level=3
shaders/enabled=true
